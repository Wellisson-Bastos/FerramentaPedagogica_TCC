@model FerramentaPedagogica.Areas.SessaoJogo.Models.Ranking.RankingViewModel;

@{
    ViewData["Title"] = "Ranking";
}

<div class="row center-align div-titulo">
    <div class="col s12">
        <h1>Ranking</h1>
    </div>
</div>
<div class="row">
    <div class="col s12">
        <ul class="collection">
            @foreach (var lItem in Model.Jogadores)
            {
                <li class="collection-item">
                    @lItem.Colocacao - @lItem.Jogador <b class="right">@lItem.Pontuacao</b>
                </li>
            }
        </ul>
    </div>
</div>
<link rel="stylesheet" href="~/css/Sessao/Ranking/Ranking.css" />